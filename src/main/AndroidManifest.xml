<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartpos.emvsample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CLOUDPOS_MSR" />
    <uses-permission android:name="android.permission.CLOUDPOS_PRINTER" />
    <uses-permission android:name="android.permission.CLOUDPOS_SMARTCARD" />
    <uses-permission android:name="android.permission.CLOUDPOS_CONTACTLESS_CARD" />
    <uses-permission android:name="android.permission.CLOUDPOS_PIN_GET_PIN_BLOCK" />
    <uses-permission android:name="android.permission.CLOUDPOS_PIN_MAC" />
    <uses-permission android:name="android.permission.CLOUDPOS_PIN_ENCRYPT_DATA" />
    <uses-permission android:name="android.permission.CLOUDPOS_PIN_UPDATE_MASTER_KEY" />
    <uses-permission android:name="android.permission.CLOUDPOS_PIN_UPDATE_USER_KEY" />
    <uses-permission android:name="android.permission.CLOUDPOS_LED" />
    <uses-permission android:name="android.permission.CLOUDPOS_SAFE_MODULE" />
    <uses-permission android:name="android.permission.CLOUDPOS_HIDE_STATUS_BAR" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".MainApp"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:extractNativeLibs="true"
        android:theme="@style/AppTheme" >

        <!-- MENU Start -->
        <activity android:name=".activity.IdleActivity" android:screenOrientation="nosensor" android:launchMode="singleInstance" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- MENU -->
        <activity android:name=".activity.FuncMenuActivity" android:screenOrientation="nosensor" />
        <!-- Process Screen -->
        <activity android:name=".activity.RequestCardActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.RemoveCardActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.ConfirmCardActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.EnquireTransActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.ErrorActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.InputAmountActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.InputOnlinePINActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.InputOfflinePINActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.ProcessEMVCardActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.ProcessOnlineActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.SelectEMVAppListActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.ShowPBOCCardRecordActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.ShowLastPBOCActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.ShowTransInfoActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".activity.TransResultActivity" android:screenOrientation="nosensor"/>

        <!-- Trans -->
        <activity android:name=".activity.Sale" />
        <activity android:name=".activity.Settle" />
        <activity android:name=".activity.QueryCardRecord" />
    </application>

</manifest>
